<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No</th>
      <th>Jenis Coin</th>
      <th>Jumlah Coin</th>
      <th>Harga Coin</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(i = 0; i < nominal.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= nominal[i].coinName %></td>
          <td><%= nominal[i].coinQuantity %></td>
          <td><%= nominal[i].coinPrice %></td>
          <td>
            <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="post">
              <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning"><i class="fas fa-pencil-alt"></i> Ubah</a>
              <button type="submit" class="btn btn-danger"><i class="fas fa-trash"></i> Hapus</button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>